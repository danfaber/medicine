(function(){

    angular.module("medicine").factory("pickListConfiguration", ["pickListEntity", pickListConfiguration]);

    function pickListConfiguration(pickListEntity){

        var defaultPickLists = [
            new pickListEntity.PickList(1,'Symptoms',[
                new Tab(1, 'Heart', [
                    new pickListEntity.TabValue('Upper Heart', 0),
                    new pickListEntity.TabValue('Middle Heart', 0),
                    new pickListEntity.TabValue('Lower Heart', 0),
                    new pickListEntity.TabValue('Related Cardiac', 0)
                ]),
                new Tab(2, 'Lung', [
                    new pickListEntity.TabValue('Left Lung', 0),
                    new pickListEntity.TabValue('Right Lung', 0),
                    new pickListEntity.TabValue('Top Lung', 0)
                ])
            ]),
            new pickListEntity.PickList(2,'Location',[
                new pickListEntity.Tab(2, 'All', [
                    new pickListEntity.TabValue('London', 0),
                    new pickListEntity.TabValue('Manchester', 0),
                    new pickListEntity.TabValue('Liverpool', 0),
                    new pickListEntity.TabValue('Loughborough', 0)
                ])
            ])
        ];

        return {
            defaultPickLists: defaultPickLists
        };
    }
})();